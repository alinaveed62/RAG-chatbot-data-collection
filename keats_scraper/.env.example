# KEATS Scraper Configuration
# Copy this file to .env and fill in your values

# KEATS Course URL
KEATS_COURSE_URL=https://keats.kcl.ac.uk/course/view.php?id=130212

# Cookie encryption key (generate with: python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())")
COOKIE_ENCRYPTION_KEY=

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Rate limiting
REQUESTS_PER_MINUTE=20
MIN_DELAY_SECONDS=2
MAX_DELAY_SECONDS=5

# Chunking configuration
CHUNK_SIZE=512
CHUNK_OVERLAP=50
